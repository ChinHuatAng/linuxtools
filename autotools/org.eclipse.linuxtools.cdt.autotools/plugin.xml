            finalPerspective="org.eclipse.cdt.ui.CPerspective"
            icon="icons/ac16/convert-normal.gif"
            id="org.eclipse.linuxtools.cdt.autotools.wizard1"
            name="%ConvertWizard.name">
         <description>
            %ConvertWizard.description
