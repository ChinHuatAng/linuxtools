     <extension point="org.eclipse.ui.actionSets">
                <actionSet
                      id="org.eclipse.linuxtools.cdt.autotools.actionSets"
                      label="%InvokeAutotools.label"
                      visible="true">
                <action
                      class="org.eclipse.linuxtools.cdt.autotools.actions.InvokeAutoconfAction"
                      enablesFor="1"
                      id="org.eclipse.linuxtools.cdt.autotools.actions.invokeAutoconf"
                      label="%InvokeAutoconf.label"
                      menubarPath="project/">
                   <enablement>
                      <objectState
                            name="projectNature"
                            value="org.eclipse.linuxtools.cdt.autotools.autotoolsNature">
                      </objectState>
                   </enablement>
		        </action>
                <action
                      class="org.eclipse.linuxtools.cdt.autotools.actions.InvokeAutomakeAction"
                      enablesFor="1"
                      id="org.eclipse.linuxtools.cdt.autotools.actions.invokeAutomake"
                      label="%InvokeAutomake.label"
