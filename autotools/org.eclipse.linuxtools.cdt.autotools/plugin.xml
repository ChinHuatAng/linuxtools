         <selection class="org.eclipse.core.resources.IProject"/>
      </wizard>
      <wizard
            category="org.eclipse.cdt.ui.newCWizards"
            class="org.eclipse.linuxtools.cdt.autotools.wizards.NewAutotoolsCProjectWizard"
